{
  "parser": "babel-eslint",
  "plugins": [ "mocha" ],
  "extends": "airbnb",
  "globals": {
    "describe": true,
    "it": true
  },
  "rules": {
    "indent": [2, 4, {"SwitchCase": 1}],
    "max-len": ["error", 120],
    "react/jsx-filename-extension": 0,
    "jsx-a11y/no-static-element-interactions": 0,
    "react/forbid-prop-types": 0
  }
}
